generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Matchmaking {
  id              String   @id @default(cuid())
  teamCode        String
  discordId   String
}

model Player {
  id           String   @id @default(cuid())
  brawlstarsId String
  discordId    String
  createdAt    DateTime @default(now())
}

model Queue {
  id        String   @id @default(cuid())
  discordId String
  brawlstarsId String
  createdAt DateTime @default(now())
}